{
  "name": "discord-event-attendance",
  "version": "1.0.0",
  "description": "Discordでイベントに出席した人を記録し、統計の表示や出席者の確認ができるBotです。",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/index.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "test \"$NODE_ENV\" != \"production\" && husky install || echo 'Skipping husky in production'",
    "heroku-postbuild": "prisma generate && prisma migrate deploy"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/node-schedule": "^2.1.7",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "csv-parse": "^5.6.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "node-schedule": "^2.1.1",
    "prisma": "^6.5.0",
    "toml": "^3.0.0",
    "tsx": "^4.19.3"
  }
}
