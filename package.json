{
  "name": "discord-event-attendance",
  "version": "1.0.0",
  "description": "Discordでイベントに出席した人を記録し、統計の表示や出席者の確認ができるBotです。",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/index.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "heroku-postbuild": "prisma generate && prisma migrate deploy"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.1",
    "@types/node-schedule": "^2.1.7",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.13"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "csv-parse": "^5.5.6",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "node-schedule": "^2.1.1",
    "prisma": "^5.14.0",
    "toml": "^3.0.0",
    "tsx": "^4.11.2"
  }
}
